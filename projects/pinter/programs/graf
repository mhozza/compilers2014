@set[1000],@n,@m,@rank[1000]

find(x,n,m,s[n],r[n]):
    s[x]=x:
        ret x
    !^:
        s[x] << find(s[x])
        ret s[x]

union(x,y,n,m,s[n],r[n]):
    fy << find(y)
    fx << find(x)
    r[fx] < r[fy]:
        s[fx]=fy
    !^:
        s[fy]=fx

>> n,m
rank << 1
for i >> [1..m]:
    >> x,y
    union(x,y)
ret << "ANO"
test << set[0]
n=0:
    ret  << "NIE"
!^:    
    for i >> [1..n]:
        find(i)!=test:
            ret << "NIE"
<< ret
